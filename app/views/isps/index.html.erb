<p id="notice"><%= notice %></p>

<h1>Isps</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Lowest price</th>
      <th>Rating</th>
      <th>Max speed</th>
      <th>Description</th>
      <th>Contact no</th>
      <th>Email</th>
      <th>Image</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @isps.each do |isp| %>
      <tr>
        <td><%= isp.name %></td>
        <td><%= isp.lowest_price %></td>
        <td><%= isp.rating %></td>
        <td><%= isp.max_speed %></td>
        <td><%= isp.description %></td>
        <td><%= isp.contact_no %></td>
        <td><%= isp.email %></td>
        <td><%= isp.image %></td>
        <td><%= isp.url %></td>
        <td><%= link_to 'Show', isp %></td>
        <td><%= link_to 'Edit', edit_isp_path(isp) %></td>
        <td><%= link_to 'Destroy', isp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Isp', new_isp_path %>
